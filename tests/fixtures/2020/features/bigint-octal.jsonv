// Test file for BigInt with octal literals (ES2020 feature)
// Demonstrates BigInt + octal (0o and 0) syntax
// Includes realistic bloat: large file permissions, modes, Unix values

{
  // Extended Unix file permissions (64-bit with BigInt)
  permissions: {
    extendedModes: {
      // Standard Unix modes as BigInt octal
      ownerReadWrite: 0o600n,
      ownerAll: 0o700n,
      groupRead: 0o040n,
      worldRead: 0o444n,
      worldReadWrite: 0o666n,
      executable: 0o755n,
      restricted: 0o700n,
      
      // Special permission bits
      setuid: 0o4000n,
      setgid: 0o2000n,
      sticky: 0o1000n,
      setuidExecutable: 0o4755n,
      setgidExecutable: 0o2755n,
      stickyDirectory: 0o1777n,
      
      // Legacy octal syntax with BigInt
      legacyOwnerReadWrite: 0600n,
      legacyExecutable: 0755n,
      legacyRestricted: 0700n
    },
    
    // Large inode numbers as octal BigInt
    inodes: {
      root: 0o2n,
